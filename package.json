{
  "name": "tutti-ch-styleguide",
  "version": "2.2.48",
  "description": "React-based styleguide for tutti.ch",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --coverage",
    "dev": "npm run styleguide",
    "start": "npm run styleguide",
    "dist:build": "npm run test",
    "dist:publish": "git pull && npm run dist:build && npm version patch -m \"Upgrade package %s\" && git push && npm publish",
    "stylelint": "stylelint 'src/**/*.scss' --syntax scss",
    "stylelint-staged": "stylelint --syntax scss",
    "styleguide": "styleguidist server",
    "styleguide:build": "npm run icons && styleguidist build",
    "svgo": "find src/styles -type f ! -name '_sprite.svg' -name '*.svg' -print0 | xargs -0 -n 1 -P 6 svgo",
    "svgo-generic": "find src/styles/Icons/assets/generic -type f ! -name '_sprite.svg' -name '*.svg' -print0 | xargs -0 -n 1 -P 6 svgo --config=.svgo.yml",
    "icons": "npm run svgo && npm run svgo-generic && node ./scripts/generateIcons.js",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "prettier --parser postcss --write",
      "stylelint-staged",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tutti-ch/react-styleguide.git"
  },
  "keywords": [
    "react",
    "tutti.ch",
    "styleguide"
  ],
  "author": "Tutti AG Switzerland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tutti-ch/react-styleguide/issues"
  },
  "homepage": "https://github.com/tutti-ch/react-styleguide#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-export-default-from": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-plugin-react-intl": "^3.0.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "extract-css-chunks-webpack-plugin": "^3.2.0",
    "file-loader": "^2.0.0",
    "git-hooks": "^1.1.9",
    "glob": "^7.1.3",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.3",
    "react-dev-utils": "^6.0.4",
    "react-styleguidist": "^7.3.10",
    "regenerator-runtime": "^0.12.1",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.6.0",
    "svg-sprite-generator": "0.0.7",
    "svgo": "^1.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "es6-promise": "^4.2.5",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "save": "^2.3.1",
    "svg4everybody": "^2.1.9"
  }
}
