{
  "name": "tutti-ch-styleguide",
  "version": "1.2.18",
  "description": "React-based styleguide for tutti.ch",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --coverage",
    "dev": "npm run styleguide",
    "dist:build": "npm run test && NODE_ENV=umd webpack",
    "dist:publish": "git pull && npm run dist:build && npm version patch -m \"Upgrade package %s\" && git push && npm publish",
    "stylelint": "stylelint 'src/**/*.scss' --syntax scss",
    "stylelint-staged": "stylelint --syntax scss",
    "styleguide": "node ./scripts/generateIcons.js && styleguidist server",
    "styleguide:build": "node ./scripts/generateIcons.js && styleguidist build",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "prettier --parser postcss --write",
      "stylelint-staged",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tutti-ch/react-styleguide.git"
  },
  "keywords": [
    "react",
    "tutti.ch",
    "styleguide"
  ],
  "author": "Tutti AG Switzerland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tutti-ch/react-styleguide/issues"
  },
  "homepage": "https://github.com/tutti-ch/react-styleguide#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "git-hooks": "^1.1.9",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^5.0.0",
    "node-sass": "^4.6.1",
    "prettier": "^1.8.2",
    "regenerator-runtime": "^0.11.0",
    "sass-loader": "^6.0.6",
    "stylelint": "^8.2.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.6.0",
    "save": "^2.3.1",
    "susy": "^2.2.12",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-styleguidist": "^6.0.33"
  }
}
